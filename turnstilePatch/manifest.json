{
    "manifest_version": 2,
    "name": "Turnstile Patcher",
    "version": "2",
    "content_scripts": [
        {
            "js": [
                "./script.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ]
}